#include "push_swap.h"

void radix_sort(t_stack **stack_a, t_stack **stack_b, int size, int len)
{
	int	size_a;
	int	size_b;
	int	j;
	char *solution;

	j = 0;
	solution = "";
	while (size_a)
	{
		while ((*stack_a)->bin[len] != '0')
		{
			rotate(stack_a);
			j++;
		}
		if (j < size / 2)
		{
			while(j)
			{
				solution = ft_strjoin(solution, "rra\n");
				free(solution)
				j--;
			}
		}
		size_a--;
	}
}
